      <TextField fx:id="filtroArticolo" layoutX="10.0" layoutY="30.0" prefHeight="30.0" prefWidth="280.0" />
      <Button fx:id="searchBtn" layoutX="300.0" layoutY="30.0" mnemonicParsing="false" onAction="#filterOrder" prefHeight="30.0" prefWidth="100.0" text="Cerca">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
      <TableView fx:id="tableStorico" layoutX="10.0" layoutY="70.0" prefHeight="520.0" prefWidth="880.0">
         <columns>
            <TableColumn fx:id="codiceColumn" prefWidth="110.0" text="Codice" />
            <TableColumn fx:id="fornitoreColumn" prefWidth="216.0" text="Fornitore" />
            <TableColumn fx:id="nomeColumn" prefWidth="195.0" text="Nome" />
            <TableColumn fx:id="dataColumn" minWidth="0.0" prefWidth="205.0" text="Data" />
            <TableColumn fx:id="numeroPezziColumn" minWidth="0.0" prefWidth="153.0" text="NumeroPezzi" />
         </columns>
      </TableView>
   </children>
</Pane>
